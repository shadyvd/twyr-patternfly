{
  "name": "twyr-patternfly",
  "version": "0.0.0-development",
  "description": "Design System for Twy'r - Ember Octane implementation of the Red Hat Patternfly Design System.",
  "license": "MITNFA",
  "readmeFilename": "README.md",
  "keywords": [
    "ember",
    "ember-addon",
    "octane",
    "twyr",
    "design",
    "system",
    "patternfly",
    "red hat",
    "vega"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/twyr/twyr-patternfly"
  },
  "bugs": {
    "url": "https://github.com/twyr/twyr-patternfly/issues",
    "email": "shadyvd@hotmail.com"
  },
  "author": {
    "name": "Vish Desai",
    "email": "shadyvd@hotmail.com",
    "url": "https://github.com/shadyvd"
  },
  "maintainers": [
    {
      "name": "Vish Desai",
      "email": "shadyvd@hotmail.com",
      "url": "https://github.com/shadyvd"
    }
  ],
  "contributors": [
    {
      "name": "Vish Desai",
      "email": "shadyvd@hotmail.com",
      "url": "https://github.com/shadyvd"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:scss": "npx stylelint app/**/*.scss",
    "lint:hbs": "npx ember-template-lint .",
    "lint:js": "npx eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "npx ember test",
    "test:ember-compatibility": "npx ember try:each",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "ember-arg-types": "^0.2.1",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.17.0",
    "ember-cli-addon-docs": "^0.6.16",
    "ember-cli-addon-docs-esdoc": "^0.2.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-deploy-lightning-pack": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-stylelint": "2.2.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-concurrency": "^1.1.7",
    "ember-concurrency-retryable": "^0.7.0",
    "ember-css-transitions": "^1.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-intl": "^4.3.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-modifier": "^1.0.3",
    "ember-prop-modifier": "^0.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.18.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.5.2",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^8.4.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "loader.js": "^4.7.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0",
    "semantic-release": "^17.0.6",
    "stylelint": "^13.3.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.17.0",
    "tracked-built-ins": "^1.0.1"
  },
  "engines": {
    "node": ">= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --signoff --hook || true",
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npx lint-staged",
      "pre-push": "npx lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "homepage": "https://twyr.github.io/twyr-patternfly"
}
